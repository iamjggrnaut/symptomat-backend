{
  "name": "penguin-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:ci": "eslint \"{src,apps,libs,test}/**/*.ts\" -c .eslintrc.ci.js",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prettier:write": "prettier --write \"src/**/*.ts\"",
    "d-build": "docker build -t surveys-backend .",
    "d-run": "docker run --rm -it -p 8000:5000 surveys-backend",
    "dc-up": "docker-compose up",
    "dc-up:build": "docker-compose up --build",
    "dc-down": "docker-compose down",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:run": "yarn typeorm migration:run",
    "migration:rev": "yarn typeorm -- migration:revert",
    "migration:create": "yarn typeorm migration:create -n $NAME",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -n $NAME",
    "migration:run:develop": "yarn typeorm migration:run --config ormconfig.develop.json",
    "migration:run:staging": "yarn typeorm migration:run --config ormconfig.staging.json",
    "migration:run:production": "yarn typeorm migration:run --config ormconfig.production.json",
    "seeds:all": "yarn seeds -s CreateManyEntities",
    "typeorm-seeding": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js",
    "seeds": "yarn typeorm-seeding seed --configName ormconfig.json",
    "seeds:develop": "yarn typeorm-seeding seed --configName ormconfig.develop.json",
    "seeds:staging": "yarn typeorm-seeding seed --configName ormconfig.staging.json",
    "seeds:production": "yarn typeorm-seeding seed --configName ormconfig.production.json",
    "schema:sync": "yarn typeorm schema:sync",
    "schema:sync:develop": "yarn typeorm schema:sync --config ormconfig.develop.json",
    "schema:sync:staging": "yarn typeorm schema:sync --config ormconfig.staging.json",
    "schema:drop": "yarn typeorm schema:drop",
    "schema:drop:develop": "yarn typeorm schema:drop --config ormconfig.develop.json",
    "schema:drop:staging": "yarn typeorm schema:drop --config ormconfig.staging.json",
    "cp-config": "cp -a yandex/. dist/src/yandex && cp -a yandex/. dist/yandex",
    "cp-env": "cp .env dist",
    "commit": "cz",
    "postinstall": "yarn --cwd node_modules/nestjs-dataloader/ run build",
    "prepare": "is-ci || husky install",
    "postbuild": "yarn cp-config && yarn cp-env"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.1.16",
    "@hapi/joi": "^17.1.1",
    "@nestjs/axios": "^0.0.1",
    "@nestjs/common": "^11.0.11",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.0.0",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/graphql": "^7.10.6",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/swagger": "^4.8.1",
    "@nestjs/terminus": "^7.1.2",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "@sentry/node": "^6.2.5",
    "@types/lodash": "^4.14.172",
    "apollo-server-express": "^2.24.1",
    "apple-signin-auth": "^1.4.2",
    "aws-sdk": "^2.886.0",
    "axios": "^0.21.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "express-robots-txt": "^0.5.0",
    "faker": "5.5.3",
    "fb": "^2.0.0",
    "firebase-admin": "^9.6.0",
    "google-auth-library": "^7.0.4",
    "googleapis": "^82.0.0",
    "graphql": "^15.5.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-redis-subscriptions": "^2.4.0",
    "graphql-tools": "^7.0.5",
    "http-status-codes": "^2.1.4",
    "husky": "^6.0.0",
    "in-app-purchase": "^1.11.4",
    "ioredis": "^4.26.0",
    "is-ci": "^3.0.0",
    "lodash": "^4.17.21",
    "mailgun-js": "^0.22.0",
    "moment": "^2.29.1",
    "nestjs-dataloader": "krislefeber/nestjs-dataloader#pull/42/head",
    "nestjs-i18n": "8.1.3",
    "nestjs-typeorm-paginate": "^2.6.0",
    "node-xlsx": "^0.17.2",
    "nodemailer": "^6.6.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "password-validator": "^5.1.1",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "^6.6.7",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.32",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.15",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.11",
    "@types/faker": "5.5.3",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/in-app-purchase": "^1.11.1",
    "@types/ioredis": "^4.22.3",
    "@types/jest": "26.0.22",
    "@types/mailgun-js": "^0.22.12",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.14.37",
    "@types/node-xlsx": "^0.15.2",
    "@types/nodemailer": "^6.4.2",
    "@types/passport-jwt": "^3.0.5",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-unused-imports": "^1.0.1",
    "husky": "^6.0.0",
    "import-sort": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prettier-plugin-import-sort": "^0.0.6",
    "supertest": "^6.1.3",
    "ts-jest": "26.5.4",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "validate-branch-name": "^1.0.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@purrweb/cloud-files-storage/(.*)": "<rootDir>/libs/cloud-files-storage/src/$1",
      "@purrweb/cloud-files-storage": "<rootDir>/libs/cloud-files-storage/src",
      "@purrweb/cloud-cache-storage/(.*)": "<rootDir>/libs/cloud-cache-storage/src/$1",
      "@purrweb/cloud-cache-storage": "<rootDir>/libs/cloud-cache-storage/src",
      "@purrweb/apple-auth-mobile/(.*)": "<rootDir>/libs/apple-auth-mobile/src/$1",
      "@purrweb/apple-auth-mobile": "<rootDir>/libs/apple-auth-mobile/src",
      "@purrweb/fb-auth-mobile/(.*)": "<rootDir>/libs/fb-auth-mobile/src/$1",
      "@purrweb/fb-auth-mobile": "<rootDir>/libs/fb-auth-mobile/src",
      "@purrweb/google-auth-mobile/(.*)": "<rootDir>/libs/google-auth-mobile/src/$1",
      "@purrweb/google-auth-mobile": "<rootDir>/libs/google-auth-mobile/src",
      "@purrweb/google-maps/(.*)": "<rootDir>/libs/google-maps/src/$1",
      "@purrweb/google-maps": "<rootDir>/libs/google-maps/src",
      "@purrweb/firebase/(.*)": "<rootDir>/libs/firebase/src/$1",
      "@purrweb/firebase": "<rootDir>/libs/firebase/src",
      "@purrweb/mailer/(.*)": "<rootDir>/libs/mailer/src/$1",
      "@purrweb/mailer": "<rootDir>/libs/mailer/src"
    }
  },
  "importSort": {
    ".js, .ts,": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engineStrict": true,
  "engines": {
    "node": "<=14.17.1"
  }
}
